{
  "preset": "ts-jest",
  "rootDir": "..",
  "testEnvironment": "node",
  "testMatch": ["**/?(*.)+(e2e-spec).[tj]s?(x)"],
  "passWithNoTests": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["<rootDir>/src/**/*.ts"],
  "coverageReporters": ["json-summary"],
  "coverageDirectory": "coverage"
}
