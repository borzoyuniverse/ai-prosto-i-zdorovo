{
    "name": "app-back",
    "type": "commonjs",
    "scripts": {
        "dev": "cross-env DOTENV_CONFIG_PATH=../.env NODE_OPTIONS='-r dotenv/config' nest start --watch",
		"dev:root-dotenv": "cross-env DOTENV_CONFIG_PATH=../.env node -r dotenv/config node_modules/.bin/nest start --watch",
        "build": "nest build",
        "test": "jest --config ./jest.config.ts --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --no-error-on-unmatched-pattern",
        "migrate": "ts-node --esm scripts/migrate-dev.ts",
        "seed": "ts-node --esm scripts/seed.ts",
        "db:start": "docker compose up -d db",
        "db:stop": "docker compose down"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "csv-parse": "^5.6.0",
        "pg": "^8.11.5",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.3.2",
        "@nestjs/testing": "^10.0.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.30",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    }
}
